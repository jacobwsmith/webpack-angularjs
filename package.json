{
  "name": "rtp-common-web",
  "version": "0.0.1",
  "description": "",
  "main": "entry.js",
  "scripts": {
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod --p",
    "clean-and-copy": "npm run clean-htdocs && npm run copy-files",
    "clean-htdocs": "rimraf htdocs",
    "copy-files": "",
    "test": "karma start karma.conf.js",
    "e2e": "protractor ./protractor.conf.js",
    "e2e-update": "webdriver-manager update",
    "e2e-server": "webdriver-manager start",
    "lint": "eslint ./src/app/ --ext .js --cache",
    "lint:debug": "eslint ./src/app/ --ext .js --cache --debug",
    "validate": "npm-run-all --parallel validate-webpack:* lint test",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "start": "webpack-dev-server --history-api-fallback --open --hot --inline --port 4321 --content-base htdocs --env.dev",
    "devs": "webpack-dev-server --content-base build --hot --inline --devtool eval --env.dev"
  },
  "author": "Jake Smith",
  "license": "ISC",
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "bootstrap": "^4.0.0-alpha.4",
    "font-awesome": "^4.7.0",
    "font-awesome-sass-loader": "^1.0.2"
  },
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-animate": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "angular-touch": "^1.5.8",
    "angular-ui-bootstrap": "^2.2.0",
    "angular-ui-router": "^1.0.0-beta.3",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-__coverage__": "^11.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.8.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "oclazyload": "^1.0.9",
    "protractor": "^4.0.10",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9",
    "webpack-validator": "^2.2.9"
  }
}
